
public class Card {

	private int number;
	private Card_Color color;
	
	public Card(int n, Card_Color c) {
		this.number = n;
		this.color = c;
	}

	public int getNumber() {
		return number;
	}

	public void setNumber(int number) {
		this.number = number;
	}

	public Card_Color getColor() {
		return color;
	}

	public void setColor(Card_Color color) {
		this.color = color;
	}
	
	
	
}
